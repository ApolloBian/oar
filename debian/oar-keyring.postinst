#!/bin/sh

set -e

case "${1}" in
    configure)
        apt-key add /usr/share/oar-keyring/oar.gpg || true
        apt-key add /usr/share/oar-keyring/oarmaster.gpg || true
        ;;

    abort-upgrade|abort-remove|abort-deconfigure)
        :
        ;;

    *)
        echo "postinst called with unknown argument \`${1}'" >&2
        exit 1
        ;;
esac

#DEBHELPER#

exit 0

