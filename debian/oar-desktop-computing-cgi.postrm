#! /bin/sh
# postrm script for oar-desktop-computing-cgi
#
# see: dh_installdeb(1)

set -e
#set -x 

case "$1" in
	purge|remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
		deluser --quiet www-data oar > /dev/null 2>&1 || true
    ;;
  *)
    echo "postrm called with unknown argument \`$1'" >&2
    exit 1
esac

#DEBHELPER#

exit 0
